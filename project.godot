; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[accessibility]

general/accessibility_support=2

[application]

config/name="Bread Adventure Open"
config/version="1.0.0"
run/main_scene="res://src/ui/open_screen.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.5", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false

[audio]

buses/default_bus_layout="res://src/global/bus_layout.tres"

[autoload]

Constant="*res://src/global/constant.gd"
Game="*res://src/global/game.tscn"
Util="*res://src/global/util.gd"
SoundManager="*res://src/global/sound_manager.tscn"
InputManager="*res://src/global/input_manager.gd"
UIManager="*res://src/global/ui_manager.tscn"
Config="*res://src/global/config.gd"
PlatformManager="*res://src/global/platform_manager.tscn"
Status="*res://src/global/status.gd"
Achievement="*res://src/global/achievement.gd"
GameLogger="*res://src/global/game_logger.gd"

[compression]

formats/zstd/compression_level=22
formats/zlib/compression_level=9
formats/gzip/compression_level=9

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=384
window/size/viewport_height=216
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=2

[file_customization]

folder_colors={
"res://asset/": "yellow",
"res://src/global/": "red",
"res://src/scene/": "green",
"res://src/unit/character/": "orange",
"res://src/unit/character/enemy/": "pink",
"res://src/unit/character/enemy/behavior/": "green",
"res://src/unit/character/enemy/boss/": "purple",
"res://src/unit/character/enemy/boss/behavior/": "green",
"res://src/unit/character/player/": "yellow"
}

[filesystem]

import/fbx2gltf/enabled=false
import/blender/enabled=false

